# merging two data 

import pandas as pd
d1 = {'Name': ['sona','sopitha','keerthi'], 'registerno': [21602167, 21602186, 21602187], 'deptname': ['CSE', 'CSE', 'IT']}
df1 = pd.DataFrame(d1)
print('DataFrame 1:\n', df1)
df2 = pd.DataFrame({'Address': ['chennai', 'chennai', 'chennai'], 'Name': ['sona', 'sopitha', 'keerthi']})
print('DataFrame 2:\n', df2)
df_merged = df1.merge(df2)
print('dataset merged:\n', df_merged)
